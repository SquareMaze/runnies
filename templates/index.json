        "template--20584145715223__0447c6d8-7e6b-47be-a617-8aa70b84a8af-feature-2": {
          "type": "feature",
          "settings": {
            "image": "shopify://shop_images/img_10.png",
            "bg_color": "#ffffff",
            "bg_gradient": "",
            "text_color": "#232323",
            "heading": "Unique Collaborations",
            "text": "<p><strong>We collaborate with POD artists from the UAE and around the world! </strong>At Runnies, See Different isn’t just a tagline – it’s our way of looking at the world. We make high-quality, affordable sunnies designed for real life and real people. Whether you’re running, adventuring, chilling, or just showing up as yourself, our shades are built to keep up – and stand out.</p>",
            "show_segments": false,
            "segments": 5,
            "active_segment": "2",
            "fill_segments": true,
            "show_labels": true,
            "label_left": "",
            "label_middle": "",
