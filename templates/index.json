    "0447c6d8-7e6b-47be-a617-8aa70b84a8af": {
      "type": "key-features",
      "blocks": {
        "template--20584145715223__0447c6d8-7e6b-47be-a617-8aa70b84a8af-feature-1": {
          "type": "feature",
          "settings": {
            "image": "shopify://shop_images/IMG_4727.png",
            "bg_color": "#f7f7f7",
            "bg_gradient": "",
            "text_color": "#232323",
            "heading": "Beauty, Creativity, Strength",
            "text": "<p><em><strong>Our story is deeply personal. As an internationally-rooted family</strong></em><em> - and proud parents to three daughters, two of whom are blind - we’ve learned firsthand that different doesn’t mean less. It means more. It means more opportunities to experience beauty, use creativity, and find strength.</em></p>",
            "show_segments": false,
            "segments": 5,
            "active_segment": "4",
            "fill_segments": true,
            "show_labels": true,
            "label_left": "",
            "label_middle": "",
